        <Button Content="Ok" HorizontalAlignment="Left" Height="49" Margin="650,38,0,0" VerticalAlignment="Top" Width="101" Click="Button_Click"/>
        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Height="27" Margin="306,63,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="162"/>
        <Label Content="Результат" HorizontalAlignment="Left" Height="35" Margin="306,20,0,0" VerticalAlignment="Top" Width="162"/>
